.model small
.stack 50
.data
tb1 db 'nhap vao chuoi:$'
tb2 db 10,13,'hien thi so ky tu chuoi:$'
str db 50 dup('$')
.code
     main proc
         mov ax,@data
         mov ds,ax
         
         mov ah,9
         lea dx,tb1
         int 21h
         
         mov ah,0ah
         lea dx,str
         int 21h
         
         mov ah,9
         lea dx,tb2
         int 21h
         
         call demchuoi
         
         mov ah,4ch
         int 21h
         
    main endp
     
 demchuoi proc
    mov ah,0
    mov al,str+1
    mov cx,0
    mov bx,10
    
    lap:
        mov dx,0
        div bx
        push dx
        inc cx
        cmp ax,0
        jnz lap
    lap1:
        pop dx
        add dx,'0'
        mov ah,2
        int 21h
        loop lap1
        
        
      mov ah,4ch
      int 21h
  demchuoi endp
