model small
.stack 100
.data   
    ;Nhap phai viet dau ',' cuoi chuoi neu k KQua se bi tinh thieu so cuoi
    tb1 db "Nhap chuoi (luu y, phai co dau ',' cuoi chuoi): $"
    tb2 db 10,13,'So luong so NT co trong chuoi: $'
    muoi dw 10
    str db 100 dup('$')
    tmp dw ?
    dem dw ?  
.code
    mov ax,@data
    mov ds,ax
    
    mov ah,9
    lea dx,tb1
    int 21h
    mov ah,10
    lea dx,str
    int 21h
    mov ah,9
    lea dx,tb2
    int 21h
    
    mov cx,0
    mov cl,[str+1]
    lea si,[str+2]
    mov ax,0
    mov dem,0
    lap:
        mov dx,[si]
        cmp dl,','
        je next1
        mul muoi
        mov dx,[si]
        mov dh,0
        sub dl,'0'
        add ax,dx
        jmp next2
        next1:
            mov bx,1
            mov tmp,ax
            chia:
                mov ax,tmp
                mov dx,0
                inc bx
                div bx
                cmp dl,0
                jne chia
                cmp bx,tmp
                mov ax,0
                je tangdem
                jmp next2
            tangdem:
                inc dem
            next2:
                inc si
                loop lap
    mov ax,dem
    mov cx,0
    chiaax:
            mov dx,0
            div muoi
            add dl,'0'
            push dx
            inc cx
            cmp ax,0
            jne chiaax
    xuat:
            pop dx
            mov ah,2
            int 21h
            loop xuat
    int 20h               

end
